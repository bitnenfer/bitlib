      <Item Condition="ControlBlock!=nullptr" Name="[Weak]">ControlBlock->WeakRefCounter.Counter</Item>
      <Item Condition="ControlBlock!=nullptr" Name="[Ptr]">ControlBlock->Ptr</Item>
