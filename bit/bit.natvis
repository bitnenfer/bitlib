  <Type Name="bit::SmallSizeAllocator::FreeBlock">
    <DisplayString Condition="Next == (~0)">End</DisplayString>
    <DisplayString>{{ Next={(void*)(((uintptr_t)this &amp; ~0xFFFF)+Next),X} }}</DisplayString>
    <Expand HideRawView="1">
      <CustomListItems MaxItemsPerView="32">
        <Variable Name="Block" InitialValue="this"/>
        <Variable Name="Index" InitialValue="0" />
        <Loop Condition="Block != nullptr &amp;&amp; Block->Next != ~0xFFFF">
          <Item Name="[{Index,u}]">Block</Item>
          <Exec>Block = (FreeBlock*)(((uintptr_t)Block &amp; ~0xFFFF)+Block->Next)</Exec>
          <Exec>Index += 1</Exec>
        </Loop>
      </CustomListItems>
    </Expand>
  </Type>

