    <Expand>
      <Item Name="[Start]">BaseAddress,X</Item>
      <Item Name="[End]">(uintptr_t)BaseAddress+ReservedSize,X</Item>
      <Item Name="[CommittedBytes]">CommittedSize</Item>
      <Item Name="[ReservedBytes]">ReservedSize</Item>
    </Expand>
